<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
    <servlet-name>Upload</servlet-name>
    <servlet-class>mediavault.Viewers.Upload</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>Serve</servlet-name>
    <servlet-class>mediavault.Viewers.Serve</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>Delete</servlet-name>
  	<servlet-class>mediavault.Viewers.Delete</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Download</servlet-name>
  	<servlet-class>mediavault.Viewers.Download</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Update</servlet-name>
  	<servlet-class>mediavault.Viewers.Update</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AutoSearch</servlet-name>
  	<servlet-class>mediavault.Viewers.AutoSearch</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GenerateUploadURL</servlet-name>
  	<servlet-class>mediavault.Viewers.GenerateUploadURL</servlet-class>
  </servlet>
 
  <servlet-mapping>
    <servlet-name>Upload</servlet-name>
    <url-pattern>/upload</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Serve</servlet-name>
    <url-pattern>/serve</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>Delete</servlet-name>
  	<url-pattern>/delete</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Download</servlet-name>
  	<url-pattern>/download</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Update</servlet-name>
  	<url-pattern>/update</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AutoSearch</servlet-name>
  	<url-pattern>/autosearch</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GenerateUploadURL</servlet-name>
  	<url-pattern>/generateUploadURL</url-pattern>
  </servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
